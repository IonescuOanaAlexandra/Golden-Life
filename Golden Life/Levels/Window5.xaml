<Window x:Class="Golden_Life.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Golden_Life"
        mc:Ignorable="d"
        Title="Window5" Height="455" Width="800" KeyDown="Window_KeyDown">
    <Grid>
        <Image Source="/Resources/sycily.png" Stretch="Fill" Panel.ZIndex="-1"/>
        <MediaElement x:Name="MePlayer" Stretch="Fill" 
                      Panel.ZIndex="2" Initialized="MePlayer_Initialized" 
                      LoadedBehavior="Manual" Margin="0,0,0,-16" 
                      MediaEnded="MePlayer_MediaEnded"  MediaOpened="MePlayer_MediaOpened" />
    </Grid>
</Window>
